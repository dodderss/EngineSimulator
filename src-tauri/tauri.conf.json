{
  "productName": "Engine Simulator",
  "version": "0.1.0",
  "identifier": "engine.dodde.rs",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm start",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Engine Simulator",
        "width": 1920,
        "height": 1080,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 1280,
        "minHeight": 720,
        "decorations": false,
        "shadow": true

      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "$schema": "../node_modules/@tauri-apps/cli/schema.json"
}
